# change the object
a = [1,2,3,4]
a[2] = 'houdy'
# a = [1,2,'houdy',3,4]

# delete object at location
del a [2]
# a = [1,2,3,4]

# operation in
'c' in 'abcdefg'
# True
'c' not in 'abcdefg'
# False

